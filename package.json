{
  "name": "@aegisjsproject/http-utils",
  "version": "1.0.4",
  "description": "HTTP Utilities for @shgysk8zer0/http-server ",
  "keywords": [],
  "type": "module",
  "exports": {
    "./*.js": {
      "import": "./*.js"
    },
    "./cache": {
      "import": "./cache.js"
    },
    "./compression": {
      "import": "./compression.js"
    },
    "./cors": {
      "import": "./cors.js"
    },
    "./csp": {
      "import": "./csp.js"
    },
    "./geo": {
      "import": "./geo.js"
    },
    "./logger": {
      "import": "./logger.js"
    },
    "./rate-limit": {
      "import": "./rate-limit.js"
    },
    "./request-id": {
      "import": "./request-id.js"
    },
    "./*.mjs": {
      "import": "./*.js"
    },
    "./*.cjs": {
      "import": "./*.js"
    },
    "./*": {
      "import": "./*.js"
    }
  },
  "engines": {
    "node": ">=24.10.0"
  },
  "private": false,
  "scripts": {
    "test": "npm run lint:js && npm run run:tests",
    "start": "node --experimental-import-meta-resolve --env-file-if-exists=.env node_modules/@shgysk8zer0/http-server/cli.js -c ./http.config.js",
    "preversion": "npm test && npm run build --if-present",
    "prepare": "npm test && npm run build --if-present",
    "lint:js": "eslint .",
    "fix:js": "eslint . --fix",
    "run:tests": "node --test",
    "create:lock": "npm i --package-lock-only --ignore-scripts --no-audit --no-fund",
    "version:bump": "npm run version:bump:patch",
    "version:bump:patch": "npm version --no-git-tag-version patch && npm run create:lock",
    "version:bump:minor": "npm version --no-git-tag-version minor && npm run create:lock",
    "version:bump:major": "npm version --no-git-tag-version major && npm run create:lock"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AegisJSProject/http-utils.git"
  },
  "author": "Chris Zuber <admin@kernvalley.us>",
  "license": "MIT",
  "funding": [
    {
      "type": "librepay",
      "url": "https://liberapay.com/shgysk8zer0"
    },
    {
      "type": "github",
      "url": "https://github.com/sponsors/shgysk8zer0"
    }
  ],
  "bugs": {
    "url": "https://github.com/AegisJSProject/http-utils/issues"
  },
  "homepage": "https://github.com/AegisJSProject/http-utils#readme",
  "devDependencies": {
    "@aegisjsproject/dev-server": "^1.0.5",
    "@shgysk8zer0/eslint-config": "^1.0.4",
    "@shgysk8zer0/http-server": "^1.1.1",
    "@shgysk8zer0/importmap": "^1.5.1",
    "eslint": "^9.39.1"
  }
}
